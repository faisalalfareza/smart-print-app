        <LinearLayout
            android:id="@+id/ProfileDescLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingBottom="40dp"
            android:paddingLeft="30dp"
            android:paddingRight="30dp">

            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="visible">

                <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentTop="true"
                    android:orientation="vertical"
                    android:visibility="visible">

                    <customfonts.MyTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#434A5E"
                        android:textSize="30dp"
                        android:textStyle="bold"
                        android:text="Congratulations!"
                        android:layout_gravity="center_vertical"
                        android:gravity="center" />

                    <customfonts.MyTextView
                        android:id="@+id/UserName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#434A5E"
                        android:textSize="18dp"
                        android:gravity="center" />

                </LinearLayout>

            </LinearLayout>

            <customfonts.MyTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:textColor="#BEBEBE"
                android:text="You have successfully request your printing document in Maestro Printing"
                android:textSize="22dp"
                android:gravity="center" />

            <Button
                android:id="@+id/done"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#ee5879"
                android:textColor="#ffffff"
                android:textStyle="bold"
                android:text="DONE"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="30dp" />

            <Button
                android:id="@+id/documentHistory"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@android:color/transparent"
                android:textColor="#ee5879"
                android:text="VIEW DOCUMENT HISTORY"
                android:textStyle="bold" />

